<style>
    .SectionRamadan .PhotoGallerySection .PhotoGalleryDetails {
        display: -webkit-box;
        display: -ms-flexbox;
        -webkit-box-orient: vertical;
        -webkit-box-direction: normal;
        -ms-flex-direction: column;
        flex-direction: column;
        position: absolute;
        top: 0;
        left: 0;
        margin-top: 7%;
        margin-left: 7%;
        max-width: 20%;
    }

    .grand-egyptian-museum {
        position: relative;
        background: url('https://betamedia.experienceegypt.eg/media/experienceegypt/img/GEM Birdseye View - Copyright Grand Egyptian Museum.JPG') no-repeat center center;
        background-size: cover;
        color: white;
        min-height: 680px;
        display: flex;
        align-items: center;
        justify-content: center;
    }

        .grand-egyptian-museum .content {
            text-align: center;
            margin-bottom: 2rem;
        }

            .grand-egyptian-museum .content h2 {
                font-size: 2rem;
                text-transform: uppercase;
                margin: 0;
                letter-spacing: 2px;
            }

            .grand-egyptian-museum .content h1 {
                font-size: 4rem;
                text-transform: uppercase;
                margin: 0.5rem 0;
            }

    .countdown {
        display: flex;
        gap: 1.5rem;
        flex-wrap: wrap;
        justify-content: center;
        position: absolute;
        bottom: 0;
        right: 0;
        margin-bottom: 3%;
        margin-right: 7%;
        /*        margin-top: 25%;
        margin-left: 10%;*/
    }

    .countdown-item {
        background: rgba(0, 0, 0, 0.5);
        padding: 0.5rem 1rem;
        text-align: center;
        border-radius: 1px;
        min-width: 95px;
    }

        .countdown-item:nth-child(1) {
            background-color: #28d0dc; /* blue */
        }

        .countdown-item:nth-child(2) {
            background-color: #fae168; /* yellow */
        }

        .countdown-item:nth-child(3) {
            background-color: #f6533e; /* red-orange */
        }

        .countdown-item:nth-child(4) {
            background-color: #1034a6; /* same as minutes for demonstration */
        }

        .countdown-item .number {
            font-size: 2.5rem;
            font-weight: bold;
            display: block;
        }

