<script>
    document.addEventListener('DOMContentLoaded', function () {
        // Select the modal element using its ID
        var myModalEl = document.getElementById('exampleModal');

        // Create a new Bootstrap Modal instance (required for Bootstrap 5)
        var modal = new bootstrap.Modal(myModalEl);

        // Call the show method
        modal.show();
        document.body.style.paddingRight = 0;
        document.body.style.overflow = 'auto';

    });

    // 1. Get references to the button and the input field
    const copyButton = document.getElementById('copyBtn');
    const textInput = document.getElementById('linkToCopy');

    // 2. Add a click event listener to the button
    copyButton.addEventListener('click', () => {

        // 3. Get the text from the input field
        const text = textInput.text.trim();

        // 4. Use the modern Clipboard API to write the text
        navigator.clipboard.writeText(text)
            .then(() => {
                // --- SUCCESS ---

                // 5. Change the button text
                copyButton.textContent = 'Copied!';

                // 6. Disable the button to prevent multiple clicks
                copyButton.disabled = true;

                // 7. Set a timer to change the button back after 5 seconds
                setTimeout(() => {
                    copyButton.textContent = 'Copy';
                    copyButton.disabled = false; // Re-enable the button
                }, 4000); // 5000 milliseconds = 5 seconds
            })
            .catch(err => {
                // --- ERROR ---
                // Log an error to the console if copying fails
                console.error('Failed to copy text: ', err);
            });
    });
</script>



