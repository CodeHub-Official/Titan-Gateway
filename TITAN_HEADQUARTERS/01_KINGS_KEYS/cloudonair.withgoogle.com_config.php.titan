      .show-auth-error #auth-error {
        display: flex;
      }
      .boxed-takeover .box {
        box-sizing: border-box;
        max-width: 450px;
        box-shadow: 0 0 4px rgba(0, 0, 0, 0.3);
        text-align: center;
        padding: 40px;
        background-color: #fff;
        margin: auto;
      }
      .boxed-takeover .box.auth-box {
        width: calc(100% - 240px);
