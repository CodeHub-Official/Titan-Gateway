<script>
    document.addEventListener("DOMContentLoaded", () => {

        $('#_btnAddNewLetter').on("click", function () {
            let valid = true;
            $("#_newsLetterForm").find('[required]').each(function () {
                if ($(this).is(':invalid') || !$(this).val()) {
                    $(this).addClass("border-danger");
                    valid = false;

                    // before make change to input text
                    if (!$(this).val())
                        $("#_errorMSG").text("Error! Enter your email.");
                    else
                        $("#_errorMSG").text("Error! Your email is not correct.")

                    // text change
                    $(this).on("change", function () {
                        if (!$(this).val()) {
                            $(this).addClass("border-danger");
                            $("#_errorMSG").text("Error! Enter your email.");
                        }
                        else {
                            $(this).removeClass("border-danger");
                            $("#_errorMSG").text("");
                        }

                        if ($(this).attr("type") == "email") {
                            if ($(this).is(':invalid')) {
                                $(this).siblings(".d-none").removeClass("d-none");
                                $(this).addClass("border-danger");
                                $("#_errorMSG").text("Error! Your email is not correct.")
                            }
                            else {
                                $(this).siblings(".text-danger").addClass("d-none");
                                $("#_errorMSG").text("");
                            }

                        }
                    });
                }
                else {
                    $(this).removeClass("border-danger");
                    $("#_errorMSG").text("");
                }
            })
            if (valid) {

                // loading
                $("#_btnAddNewLetter").addClass("disabled");
                $("#_btnAddNewLetter").find("span").first().removeClass("d-none");

                $.ajax({
                    type: "Post",
                    url: "/Home/AddNewsLetterSubscrib?email=" + $("#_newLetterText").val(),
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (response) {
                        if (response.type === 1)
                            toastr.success("Well done! You have subscribed to our newsletter successfully!");
                        else
                            toastr.warning("Error! Your email has been already registered.");

                        // end loading
                        $("#_btnAddNewLetter").removeClass("disabled");
                        $("#_btnAddNewLetter").find("span").first().addClass("d-none");
                    },
                    failure: function (response) {
                        alert(response.d);
                    },
                    error: function (response) {
                        alert(response.d);
                    }
                });

            }
        });
    });
</script>

