    <title>404</title><meta property='og:title' content='404'><style>
        * {
            
            box-sizing: border-box;
            text-decoration: none !important;
        }


        .z-100 {display: none !important;}


        .the-design {
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            min-height: 100vh;
            background-color: #fffff;
            padding: 20px;
        }

        .logo {
            width: 150px;
            margin-bottom: 40px;
        }

        .content {
            text-align: center;
            color: #2c2d65;
        }

        h1 {
            font-size: 2.5rem !important;
            margin-bottom: 0px !important;
            font-weight: 600;
            color: #2c2d65 !important;
        }

        .subtitle {
            font-size: 1.2rem !important;
            margin-bottom: 40px;
            opacity: 0.9;
        }

        .buttons {
            display: flex;
            gap: 20px;
            flex-wrap: wrap;
            justify-content: center;
        }

        .button {
            padding: 15px 30px;
            border-radius: 50px;
            text-decoration: none;
            font-weight: 600;
            transition: all 0.3s ease;
            display: flex;
            align-items: center;
            gap: 10px;
            cursor: pointer;
            border: none;
        }

        .button-primary {
            background-color: #2c2d65;
            color: #ffffff;
        }

        .button-secondary {
            background-color: transparent !important;
            color: #2c2d65 !important;
            border: 2px solid #2c2d65 !important;
        }

        .button:hover {
            transform: translateY(-2px);
            box-shadow: 0 4px 15px rgba(0, 0, 0, 0.2);
        }

        .popup-overlay {
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: rgba(0, 0, 0, 0.5);
            display: none;
            justify-content: center;
            align-items: center;
            z-index: 1000;
        }

        .popup {
            background: white;
            border-radius: 20px;
            padding: 30px;
            width: 90%;
            max-width: 500px;
            position: relative;
        }

        .close-button {
            position: absolute !important;
            top: 15px !important;
            right: 15px !important;
            background: none !important;
            border: none !important;
            font-size: 24px !important;
            cursor: pointer !important;
            color: #2c2d65 !important;
        }

        .popup-header {
            color: #2B2B54;
            font-size: 1.5rem !important;
            font-weight: 600;
            margin-bottom: 25px;
            padding-bottom: 15px;
            border-bottom: 2px solid #F5F5FF;
        }

        .categories {
            display: grid;
            grid-template-columns: repeat(2, 1fr);
            gap: 20px;
            margin-bottom: 30px;
        }

        .category {
            background: #F5F5FF;
            border-radius: 15px;
            padding: 20px;
            text-align: center;
            cursor: pointer;
            transition: all 0.3s ease;
            text-decoration: none;
        }

        .category:hover {
            transform: translateY(-2px);
            box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1);
        }

        .category-icon {
            font-size: 24px;
            margin-bottom: 10px;
            color: #2c2d65;
            width: 32px;
            text-align: center;
            margin: 0 auto;
        }

        .category-name {
            color: #2B2B54;
            font-weight: 600;
        }

        .menu-items {
            display: flex;
            flex-direction: column;
            gap: 0px;
        }

        .menu-item {
            display: flex;
            justify-content: space-between;
            align-items: center;
            padding: 15px;
            border-bottom: 1px solid #F5F5FF;
            color: #2c2d65;
            cursor: pointer;
            text-decoration: none;
            transition: all 0.3s ease;
        }

        .menu-item:hover {
            background: #F5F5FF;
            border-radius: 10px;
        }

        .arrow-icon {
            color: #2B2B54;
        }

        @media (max-width: 600px) {
            .buttons {
                flex-direction: column;
                width: 100%;
            }

            .button {
                width: 100%;
                justify-content: center;
            }

            

            h1 {
                font-size: 2rem;
            }
        }
    </style>


<script>
        const categories = [
            { name: 'Shop', icon: '<img src="/images/passengerslibraries/errors/shop.svg">', url: '/experiences/shops' },
            { name: 'Dine', icon: '<img src="/images/passengerslibraries/errors/dine.svg">', url: '/experiences/restaurants' },
            { name: 'Relax', icon: '<img src="/images/passengerslibraries/errors/relax-icon-blue-01.svg">', url: '/experiences/relax---refresh' },
            { name: 'Services', icon: '<img src="/images/passengerslibraries/errors/services.svg">', url: '/experiences/services' }
        ];

        const menuItems = [
            { name: 'Flight status', url: '/flight-status' },
            { name: 'Passport control & visas', url: '/information/immigration-visas' },
            { name: 'Metro', url: '/transport/metro' },
            { name: 'Lost & found', url: '/lost-and-found' },
            { name: 'Contact us', url: '/contact-information' }
        ];

        document.addEventListener('DOMContentLoaded', () => {
            const destinationBtn = document.getElementById('destinationBtn');
            const popupOverlay = document.getElementById('popupOverlay');
            const closePopup = document.getElementById('closePopup');
            const categoriesContainer = document.getElementById('categoriesContainer');
            const menuItemsContainer = document.getElementById('menuItemsContainer');

            // Populate categories
            categoriesContainer.innerHTML = categories.map(category => `
                <a href="${category.url}" class="category">
                    <div class="category-icon">${category.icon}</div>
                    <div class="category-name">${category.name}</div>
                </a>
            `).join('');

            // Populate menu items
            menuItemsContainer.innerHTML = menuItems.map(item => `
                <a href="${item.url}" class="menu-item">
                    ${item.name}
                    <span class="arrow-icon">â†’</span>
                </a>
            `).join('');

            destinationBtn.addEventListener('click', () => {
                popupOverlay.style.display = 'flex';
            });

            closePopup.addEventListener('click', () => {
                popupOverlay.style.display = 'none';
            });

            popupOverlay.addEventListener('click', (e) => {
                if (e.target === popupOverlay) {
                    popupOverlay.style.display = 'none';
                }
            });
        });
    </script><link rel='canonical' href='https://dubaiairports.ae/error-pages/404' />

