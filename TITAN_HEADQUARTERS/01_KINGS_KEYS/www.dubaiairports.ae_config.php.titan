                <script>
                    function getAdvancedConfig() {

                                           const surveyModalOverlay = document.getElementById(
                      "survey-modal-overlay"
                    );
                    const surveyIframe = document.getElementById("survey-iframe");
                    const closeSurveyModalButton =
                      document.getElementById("close-survey-modal");

                    function hideSurveyIframe() {
                      surveyModalOverlay.classList.remove("visible");
                      // Clear src after transition to prevent content from showing prematurely
                      setTimeout(() => {
                        surveyIframe.src = "";
                      }, 300); // Matches CSS transition duration
                      //debug.log("Survey iframe closed.");
                    }

                    function showSurveyIframe(url) {
                      surveyIframe.src = url;
                      surveyModalOverlay.classList.add("visible");
                    }

                    // Add event listener for the close button on the survey modal
                    closeSurveyModalButton.addEventListener("click", hideSurveyIframe);

                    // Ensure the survey modal is hidden initially on page load
                    surveyModalOverlay.classList.remove("visible");

                    // Get the elements
                    const chatButton = document.getElementById("chatButton");
                    const chatForm = document.getElementById("chatForm");
                    const userChatForm = document.getElementById("userChatForm");
                    const minimizeChatForm = document.getElementById("minimizeChatForm");
                    const chatFormBtns = document.getElementById("chatFormBtns");
                    const closeButton = chatForm.querySelector(".close-chat");
                    const minimizeButton = chatForm.querySelector(".minimize-chat");
                    const startChatButton = document.getElementById("startChatButton");
                    const loadingSpinner = document.getElementById("loadingSpinner");

                    // New elements for minimized chat bar
                    const minimizedChatBar = document.getElementById("minimizedChatBar");
                    const closeMinimizedChatButton =
                      document.getElementById("closeMinimizedChat");

                    let hasChatBeenOpened = false; // Flag to track if a chat has been opened in the current session
                    let genesysScriptLoaded = false; // Flag to track if the Genesys script has been loaded
                    let genesysMessengerReady = false; // NEW: Flag to track full Genesys Messenger readiness
                    let pendingChatData = null; // NEW: To store form data until Genesys is ready

                    async function initializeGenesysMessenger() {
                      var response = await fetch('/MainSite/Chat/GetData');
                      var data = await response.json();
                      if (genesysScriptLoaded) {
                        return;
                      }

                      (function (g, e, n, es, ys) {
                        g["_genesysJs"] = e;
                        g[e] =
                          g[e] ||
                          function () {
                            (g[e].q = g[e].q || []).push(arguments);
                          };
                        g[e].t = 1 * new Date();
                        g[e].c = es;
                        ys = document.createElement("script");
                        ys.async = 1;
                        ys.src = n;
                        ys.charset = "utf-8";
                        document.head.appendChild(ys);
                      })(
                        window,
                        "Genesys",
                        data.dataURL,
                        {
                          environment:  data.environment,
                          deploymentId:  data.deploymentKey,
                          autoOpen: true,
                          launcher: {
                            show: false,
                          },
                        }
                      );

                      genesysScriptLoaded = true; // Mark script as loaded

                      // Attach event listeners for Genesys once the snippet is run
                      // These subscriptions queue up even if Genesys isn't fully ready yet.
                      window.Genesys("subscribe", "Messenger.ready", function () {
                        genesysMessengerReady = true; // Set the new readiness flag

                        // NEW: If there's pending chat data, now is the time to open the chat
                        if (pendingChatData) {
                          openGenesysChat(pendingChatData.name, pendingChatData.email);
                          pendingChatData = null; // Clear pending data
                        }
                      });

                      window.Genesys("subscribe", "Messenger.opened", function () {
                        hasChatBeenOpened = true; // Mark that a chat has been opened
                        // NEW: Hide custom form and spinner after chat successfully opens
                        if (!chatForm.classList.contains("hidden")) {
                          chatForm.classList.add("hidden");
                        }
                        loadingSpinner.classList.add("hidden");
                        startChatButton.disabled = false;
                        // Hide minimized chat bar if it was shown
                        minimizedChatBar.classList.add("hidden");
                      });

                      window.Genesys("subscribe", "Messenger.closed", function () {
                        // Show the main chat button when Genesys Messenger UI is closed
                        // Check if minimized bar should be shown instead
                        if (hasChatBeenOpened) {
                          minimizedChatBar.classList.remove("hidden");
                        } else {
                          minimizedChatBar.classList.add("hidden"); // Ensure minimized bar is hidden if no chat was opened
                        }
                      });

                      window.Genesys(
                        "subscribe",
                        "MessagingService.conversationCleared",
                        function (response) {
                          console.log(
                            "Genesys conversation was cleared by the user!",
                            response
                          );
                          hasChatBeenOpened = false; // Reset the flag
                          // chatForm.classList.remove("hidden"); // Show your custom form immediately
                          // reset form fields here
                          const nameInput = document.getElementById("name");
                          const emailInput = document.getElementById("email");
                          if (nameInput) nameInput.value = "";
                          if (emailInput) emailInput.value = "";
                          // Hide minimized chat bar if conversation is cleared
                          minimizedChatBar.classList.add("hidden");
                        }
                      );

                      window.Genesys(
                        "subscribe",
                        "MessagingService.messagesReceived",
                        function (response) {

                          const direction = response.data.messages.at(-1).direction;
                          const message = response.data.messages.at(-1).text;

                          // direction is Outbound relative to Genesys
                          if (direction == "Outbound") {
                            // check if msg contains Dear CUSTOMERNAME, please give us your feedback on the following survey https://da.hivecfm.online/?q=Hj29Ee2Hdq%2bR6RWGi9ss7zEqhiF5guLpsPy9xTY8ZbA%3d

                            const pattern = "https://da.hivecfm.online/";

                            // Using a regular expression to find the link
                            const regex = /https:\/\/da\.hivecfm\.online\/[^\s]+/g;
                            const match = message.match(regex);
                            let surveyUrl;
                            if (match) {
                              const link = match[0];
                              surveyUrl = link;
                              // Close Genesys Messenger UI
                              closeGenesysMessengerUI();

                              showSurveyIframe(surveyUrl);

                              //todo: clear conversation after receiving url
                              window.Genesys(
                                // Use window.Genesys for clarity
                                "command",
                                "MessagingService.clearConversation",
                                {},
                                function (data) {
                                  /*fulfilled callback*/
                                  console.log("Conversation cleared.", data);
                                },
                                function (error) {
                                  /*rejected callback*/
                                  console.log("Failed to clear conversation.", error);
                                }
                              );
                            } else {
                              console.log(
                                "Message does not contain the link starting with the specified pattern."
                              );
                            }
                          }
                        }
                      );
                    }

                    // NEW: Extracted function to open Genesys chat with user data
                    function openGenesysChat(name, email) {
                      // Set Genesys attributes for the new conversation
                      window.Genesys("command", "Database.set", {
                        messaging: {
                          customAttributes: {
                            Name: name,
                            Email: email,
                          },
                        },
                      });
                      // Open Genesys Messenger. This will initiate a new conversation.
                      window.Genesys("command", "Messenger.open");
                    }

                    // Function to close Genesys Messenger UI
                    function closeGenesysMessengerUI() {
                      // Use window.Genesys for clarity
                      if (typeof window.Genesys === "function") {
                        window.Genesys("command", "Messenger.close");
                      } else {
                        // Changed warning to reflect that Genesys might not be loaded if not yet initialized by form submit
                        console.warn(
                          "Genesys function not found, cannot close Messenger UI. (Expected if Genesys not yet loaded/initialized)"
                        );
                      }
                    }

                    // Event listener for the main chat toggle button
                    // Event listener for the main chat toggle button
                    if (chatForm) {
                      chatFormBtns.addEventListener("click", () => {
                         minimizeChatForm.classList.remove("hidden");
                         chatForm.classList.add("chat-max-height");
                      });
                        // If a chat has already been initiated AND Genesys is loaded AND ready, open Genesys Messenger directly
                        if (
                          hasChatBeenOpened &&
                          genesysScriptLoaded &&
                          genesysMessengerReady
                        ) {
                          window.Genesys("command", "Messenger.open");
                          chatForm.classList.add("hidden"); // Ensure your custom form is hidden
                          minimizedChatBar.classList.add("hidden"); // Hide minimized bar
                        } else {
                          // If no chat has been initiated or Genesys isn't loaded/ready yet, show/hide the form
                          chatForm.classList.toggle("hidden");

                          // Ensure spinner is hidden and button enabled when form is shown
                          loadingSpinner.classList.add("hidden");
                          startChatButton.disabled = false;

                          // Ensure minimized chat bar is hidden if form is toggled
                          minimizedChatBar.classList.add("hidden");
                        }

                    }

                    // Event listener for the close button inside the chat form
                    if (closeButton && chatForm) {
                      closeButton.addEventListener("click", () => {
                        chatForm.classList.add("hidden");
                        // Show the main chat button when the form is closed, unless a chat has been opened
                        if (hasChatBeenOpened) {
                          minimizedChatBar.classList.remove("hidden");
                        } else {
                          minimizedChatBar.classList.add("hidden"); // Ensure minimized bar is hidden
                        }
                      });
                    }

                    // Event listener for the minimize button
                    if (minimizeButton && chatForm) {
                      minimizeButton.addEventListener("click", (e) => {
                          e.stopPropagation();
                        minimizeChatForm.classList.add("hidden");
                         chatForm.classList.remove("chat-max-height");
                        closeGenesysMessengerUI();
                        // Show the minimized chat bar
                        if (hasChatBeenOpened) {
                          minimizedChatBar.classList.remove("hidden");
                        }
                      });
                    }

                    // Event listener for clicking the minimized chat bar to re-open
                    if (minimizedChatBar) {
                      minimizedChatBar.addEventListener("click", () => {
                        if (
                          hasChatBeenOpened &&
                          genesysScriptLoaded &&
                          genesysMessengerReady
                        ) {
                          window.Genesys("command", "Messenger.open");
                          minimizedChatBar.classList.add("hidden"); // Hide the bar when chat is re-opened
                        } else {
                          // If for some reason Genesys isn't ready but hasChatBeenOpened is true,
                          // we should probably just show the form again.
                          chatForm.classList.remove("hidden");
                          minimizedChatBar.classList.add("hidden");
                        }
                      });
                    }

                    // Event listener for closing the minimized chat bar
                    if (closeMinimizedChatButton) {
                      closeMinimizedChatButton.addEventListener("click", (event) => {
                        event.stopPropagation(); // Prevent the click from bubbling up to the minimizedChatBar and reopening the chat
                        minimizedChatBar.classList.add("hidden");
                        hasChatBeenOpened = false; // Consider the chat session closed from the user's perspective
                        // Optionally, also clear the Genesys conversation here if the user explicitly closes the minimized bar
                        if (typeof window.Genesys === "function" && genesysMessengerReady) {
                          window.Genesys(
                            "command",
                            "MessagingService.clearConversation",
                            {},
                            function (data) {
                              console.log(
                                "Conversation cleared from minimized bar close.",
                                data
                              );
                            },
                            function (error) {
                              console.log(
                                "Failed to clear conversation from minimized bar close.",
                                error
                              );
                            }
                          );
                        }
                        // Reset form fields
                        const nameInput = document.getElementById("name");
                        const emailInput = document.getElementById("email");
                        if (nameInput) nameInput.value = "";
                        if (emailInput) emailInput.value = "";
                      });
                    }

                    document.addEventListener("DOMContentLoaded", function () {
                      // Keep the chat form and minimized bar hidden initially until clicked
                      chatForm.classList.add("hidden");
                      minimizedChatBar.classList.add("hidden");
                    });

                    // Handle form submission
                    function handleFormSubmit(event) {
                      event.preventDefault(); // Prevent default form submission

                      const nameInput = document.getElementById("name");
                      const emailInput = document.getElementById("email");
                      const name = nameInput ? nameInput.value : "";
                      const email = emailInput ? emailInput.value : "";

                      // Show loading spinner and disable button
                      loadingSpinner.classList.remove("hidden");
                      startChatButton.disabled = true;

                      // NEW: Store form data in pendingChatData
                      pendingChatData = { name, email };

                      // Initialize Genesys Messenger only when the form is submitted
                      initializeGenesysMessenger();

                      // NEW: Check if Genesys is already ready. If so, open chat immediately.
                      // Otherwise, it will be opened by the 'MessagingService.ready' subscription.
                      if (genesysMessengerReady) {
                        openGenesysChat(pendingChatData.name, pendingChatData.email);
                        pendingChatData = null; // Clear pending data as it's handled
                      } else {
                        console.log(
                          "Genesys not yet fully ready, waiting for 'MessagingService.ready' event to open chat."
                        );
                      }

                      // Removed the old `tryOpenGenesys` and its `setTimeout` entirely.
                      // The logic is now handled by `pendingChatData` and the Genesys `ready` event.
                    }

                    // Add event listener to the form for submission
                    if (userChatForm) {
                      userChatForm.addEventListener("submit", handleFormSubmit);
                    }
                    }


