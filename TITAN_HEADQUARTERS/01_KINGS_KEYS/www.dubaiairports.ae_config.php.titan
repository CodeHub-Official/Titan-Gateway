<script>
    document.addEventListener("DOMContentLoaded", function () {
        const searchInput = document.getElementById("header-search");
        const resetButton = document.getElementById("header-search-reset");

        function toggleResetButton() {
            const hasText = searchInput.value.trim().length > 0;
            if (hasText) {
                resetButton.classList.remove("hidden-reset");
                resetButton.setAttribute("aria-hidden", "false");
                resetButton.removeAttribute("tabindex");
            } else {
                resetButton.classList.add("hidden-reset");
                resetButton.setAttribute("aria-hidden", "true");
                resetButton.setAttribute("tabindex", "-1");
            }
        }

        // Initial check in case input is pre-filled
        toggleResetButton();

        // Listen for input changes
        searchInput.addEventListener("input", toggleResetButton);

        // Clear input and hide button on reset
        resetButton.addEventListener("click", function () {
            searchInput.value = "";
            toggleResetButton();
        });
    });
</script>


