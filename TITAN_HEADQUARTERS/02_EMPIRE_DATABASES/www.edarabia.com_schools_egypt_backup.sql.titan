            <script>

            jQuery(document).ready(function () {

                function submitFilterr() {
                    jQuery('input[name="page"]').val(1);
                    jQuery("form.listingFilter").submit();
                }

                // Open / Close dropdown
                jQuery(document).on("click", ".ed-custom-dropdown .dropdown-toggle", function (event) {
                    event.preventDefault();
                    var parent = jQuery(this).closest(".ed-custom-dropdown");

                    // Close all other dropdowns
                    jQuery(".ed-custom-dropdown").not(parent).removeClass("open");

                    // Toggle current dropdown
                    parent.toggleClass("open");
                });

                // Select option
                jQuery(document).on("click", ".ed-custom-dropdown .dropdown-menu li", function (e) {

                    var parentDropdown = jQuery(this).closest(".ed-custom-dropdown");
                    var filterInputName = parentDropdown.data("input");
                    /* ============================
                    MULTI-SELECT DROPDOWN ONLY
                    ============================ */
                    if (parentDropdown.hasClass("multi-select")) {
                        e.preventDefault();
                        e.stopPropagation();

                        var hiddenInput = parentDropdown
                            .closest(".ajax-filter")
                            .find("input[name='" + filterInputName + "']");

                        var label = parentDropdown.find(".dropdown-label");

                        // Toggle selected state
                        jQuery(this).toggleClass("selected");

                        var values = [];
                        var labels = [];

                        parentDropdown.find("li.selected").each(function () {
                            values.push(jQuery(this).data("value"));
                            labels.push(jQuery(this).text());
                        });

                        // Set comma-separated values
                        if (parentDropdown.data("array") === true) {

                            var container = parentDropdown
                                .closest(".ajax-filter")
                                .find("." + filterInputName + "-hidden-inputs");

                            container.empty();

                            values.forEach(function (val) {
                                container.append(
                                    '<input type="hidden" name="' + filterInputName + '[]" value="' + val + '">'
                                );
                            });
                        } else {
                            hiddenInput.val(values.join(","));
                        }

                        // Update label text
                        if (labels.length === 0) {
                            label.text(parentDropdown.data("lable"));
                        } else {
                            label.text(labels.join(", "));
                        }

                        // Fire AJAX (same behavior as others)
                        if (parentDropdown.parents(".ajax-filter").length) {
                            submitFilterr();
                        }

                        return; // ðŸ”´ stop here â€” do NOT run single-select logic
                    }

                    /* ============================
                    SINGLE-SELECT (EXISTING)
                    ============================ */

                    var value = jQuery(this).data("value");
                    var text = jQuery(this).text();

                    parentDropdown.find("li").removeClass("selected");
                    jQuery(this).addClass("selected");

                    parentDropdown.find(".dropdown-label").text(text);
                    parentDropdown.find("input[type='hidden']").val(value);
                    parentDropdown.removeClass("open");

                    //if (filterName == 'dataSortingOrder') {
                    //parentDropdown.find("input[name='sortingOrder']").val(value);
                    parentDropdown.find("input[name='" + filterInputName + "']").val(value);
                    //filterInputName
                    //}

                    if (jQuery(this).parents(".ajax-filter").length) {
                        e.preventDefault();
                        submitFilterr();
                        return;
                    }

                    if (value) {
                        window.location.href = value;
                    }
                });

                // Close dropdown when clicking outside
                jQuery(document).on("click", function (event) {
                    if (!jQuery(event.target).closest(".ed-custom-dropdown").length) {
                        jQuery(".ed-custom-dropdown").removeClass("open");
                    }
                });


            });
        </script>


