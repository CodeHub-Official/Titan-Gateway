<script>
(function() {
    'use strict';
    
    // Check if user has any login cookies (WordPress or magic link session)
    function hasLoginCookie() {
        const cookies = document.cookie;
        const hasWPCookie = cookies.includes('wordpress_logged_in_');
        const hasMagicLinkCookie = cookies.includes('magic_link_user=1');
        // Fallback: wp-settings-{user_id} cookie indicates WordPress login
        const hasWPSettings = /wp-settings-\d+/.test(cookies);
        
        return hasWPCookie || hasMagicLinkCookie || hasWPSettings;
    }
    
    // Default non-logged-in HTML for user area
    function getDefaultUserArea() {
        return '<div class="wp-block-buttons signin-btn is-content-justification-right is-layout-flex wp-container-core-buttons-is-layout-1 wp-block-buttons-is-layout-flex">' +
            '<div class="wp-block-button otpl-popup">' +
            '<a href="javascript:void(0)" class="wp-block-button__link wp-element-button">' +
            '<img decoding="async" src="https://ent.news/internal/408141.svg" style="padding: 0 4px;" alt="Sign in">' +
            'Sign in' +
            '</a></div></div>';
    }
    
    // Default non-logged-in HTML for burger menu auth
    function getDefaultBurgerAuth() {
        return '<div class="otpl-popup sign-in-btn">' +
            '<a href="javascript:void(0)"><img decoding="async" src="https://ent.news/internal/408141.svg" alt="Sign in">' +
            'Sign in</a></div>';
    }
    
    function loadHeaderContent() {
        const userArea = document.getElementById('ajax-user-area');
        const burgerMenuAuth = document.getElementById('ajax-burger-menu-auth');
        
        // If no login cookie, render default state without AJAX
        if (!hasLoginCookie()) {
            if (userArea) userArea.innerHTML = getDefaultUserArea();
            if (burgerMenuAuth) burgerMenuAuth.innerHTML = getDefaultBurgerAuth();
            return;
        }
        
        // User is logged in - fetch dynamic content
        const ajaxUrl = 'https://enterpriseam.com/egypt/wp-admin/admin-ajax.php';
        
        Promise.all([
            fetch(ajaxUrl + '?action=get_header_user_area').then(r => r.text()),
            fetch(ajaxUrl + '?action=get_burger_menu_auth').then(r => r.text())
        ]).then(function(responses) {
            if (userArea) userArea.innerHTML = responses[0];
            if (burgerMenuAuth) burgerMenuAuth.innerHTML = responses[1];
        }).catch(function(error) {
            console.error('Error loading header content:', error);
            // Fallback to default on error
            if (userArea) userArea.innerHTML = getDefaultUserArea();
            if (burgerMenuAuth) burgerMenuAuth.innerHTML = getDefaultBurgerAuth();
        });
    }
    
    // Load content when DOM is ready
    if (document.readyState === 'loading') {
        document.addEventListener('DOMContentLoaded', loadHeaderContent);
    } else {
        loadHeaderContent();
    }
})();
