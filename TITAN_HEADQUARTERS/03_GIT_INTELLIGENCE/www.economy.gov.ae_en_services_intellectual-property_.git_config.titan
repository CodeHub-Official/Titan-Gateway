//change the ministry's name
function replaceMinistryText() {
  const replacements = [
    { from: "Ministry of Economy", to: "Ministry of Economy & Tourism" },
    { from: "وزارة الاقتصاد", to: "وزارة الاقتصاد والسياحة" }
  ];

  const skipTexts = ["وزارة الاقتصاد والسياحة", "Ministry of Economy & Tourism", "Ministry of Economy and Tourism"];

  function walk(node) {
    let child = node.firstChild;
    while (child) {
      const next = child.nextSibling;
      if (child.nodeType === 3) { // Text node
        replaceInTextNode(child);
      } else if (
        child.nodeType === 1 &&
        child.nodeName !== "SCRIPT" &&
        child.nodeName !== "STYLE"
      ) {
        walk(child);
      }
      child = next;
    }
  }

  function replaceInTextNode(textNode) {
    let text = textNode.nodeValue;

    // Skip if text already contains exact target strings
    if (skipTexts.some(skip => text.includes(skip))) {
      return;
    }

    for (const { from, to } of replacements) {
      const regex = new RegExp(from, "g");
      text = text.replace(regex, to);
    }
    textNode.nodeValue = text;
  }

  walk(document.body);
}



