	$( window ).resize(function() {
      $('.news_slider_mediacenter').slick("refresh");
    });

    function renderCarousel() {
        var slideDirection = $("html").attr("dir");
        var rtl = slideDirection == "ltr" ? false : true;
        var updaterNode = $('.page_counter').find('span');
        var slickNode = $('.news_slider_mediacenter');
		var prevBtnNode = slickNode.parents('#news_slider').find('.slick-arrow.slick-prev');
		var NextBtnNode = slickNode.parents('#news_slider').find('.slick-arrow.slick-next');
        
        slickNode.on('init', function(event, slick, currentSlide, nextSlide){
			var i = (currentSlide ? currentSlide : 0) + 1;
			var slideCount = slick.slideCount;
			var slideToShow = slick.options.slidesToShow;		

            if(slideCount <= slideToShow){
				updaterNode.hide(); 
			}else{
				updaterNode.show(); 
			}
			updaterNode.text(i + '/' + slick.slideCount);				
		});

        slickNode.not('.slick-initialized').slick({
            dots: false,
            infinite: true,
            speed: 500,
            slidesToShow: 2,
            slidesToScroll: 1,
            rtl: rtl,
            arrows: false,
            responsive: [{
                    breakpoint: 1199,
                    settings: {
                        slidesToShow: 2,
                    }
                },
                {
                    breakpoint: 991,
                    settings: {
                        slidesToShow: 2,
                    }
                },
                {
                    breakpoint: 768,
                    settings: {
                        slidesToShow: 1,
                    }
                },
            ],
    
        });

        prevBtnNode.click(function() {
			slickNode.slick('slickPrev');
		});

		NextBtnNode.click(function() {
			slickNode.slick('slickNext');
		});

        slickNode.on('init reInit afterChange', function(event, slick, currentSlide, nextSlide){
			var i = (currentSlide ? currentSlide : 0) + 1;
			var slideCount = slick.slideCount;
			var slideToShow = slick.options.slidesToShow;

            if(slideCount <= slideToShow){
				updaterNode.hide(); 
			}else{
				updaterNode.show(); 
			}
			updaterNode.text(i + '/' + slick.slideCount);		
		});  
    }
</script>

		
		




			
			
	
	







	
	
					
				
			
		
	
	


