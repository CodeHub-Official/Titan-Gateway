<script type="text/javascript">
	$(document).ready(function(){

		//loadNotification();
		function loadNotification(){
			$.ajax({
				type: "POST",
				url: "https://www.moet.gov.ae/home?p_p_id=com_liferay_journal_content_web_portlet_JournalContentPortlet&p_p_lifecycle=2&p_p_state=normal&p_p_mode=view&p_p_resource_id=Notification&p_p_cacheability=cacheLevelPage&p_p_auth=r0VKxEFX",
				success : function(data) {
					var notificationData = JSON.parse(data);	
					var listHtml = '';				
					$.each(notificationData, function(i, item){
						listHtml += '<li>'+
									'<a href="' +item.url + '">' + item.title + '</a>'+
									'<i class="moeicon moeicon-exit-2 remove-notification"><span class="path1"></span><span class="path2"></span></i>'+
								'</li>';	
					});
					var dataCount = notificationData.length;	
					if(dataCount > 0){
						$('.notifications_flag').removeClass('hide');
					}				
					$(".notifications_expanded .notifications_list").html( listHtml );
					$(".item.haveNotification .notifications_flag span").html(dataCount);
					$(".notifications_expanded .notifications_container p span").html("(" + dataCount + ")");
				},
				error:function(data){
					console.log("==========error===========");
				}        
			});	
		}
		
		// Notifications		
		$(document).find('.item.haveNotification').on('click', function (){	
			$('.floating_icons_side .items_list .item').not($(this)).each(function(){
				$(this).find('.moeicon').toggleClass('dimmed');
			});
			$('.notifications_expanded').toggleClass('closed');
			$(this).find('.moeicon').toggleClass('moeicon-notification moeicon-exit-1');
		});
		
		$(document).find('.notifications_expanded .notifications_list .remove-notification').on('click', function (){	
			$(this).parent().remove();
			let notificationCount = $(".notifications_expanded .notifications_list li").length;
			$(".item.haveNotification .notifications_flag span").html(notificationCount);
			$(".notifications_expanded .notifications_container p span").html("(" + notificationCount + ")");
		});

		// Rating

		// Star Rating Manage - Start
		$('.rate_page .rating_stars .fa, .item_rating .rating_stars .fa').on('mouseenter', function () {
			$(this).parent().find('.fa').removeClass('marked');
			$(this).prevAll().addBack().addClass('marked');
		});

