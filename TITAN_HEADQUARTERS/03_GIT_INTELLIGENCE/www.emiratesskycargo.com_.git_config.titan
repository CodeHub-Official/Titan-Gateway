        // Function to get breadcrumb data from em-header component
        function getBreadcrumbData() {
            const headerComponent = document.querySelector('em-header');
            console.log('GTM Debug - Header component found:', !!headerComponent);
            
            if (headerComponent && headerComponent.hasAttribute('breadcrumb')) {
                try {
                    const breadcrumbAttr = headerComponent.getAttribute('breadcrumb');
                    console.log('GTM Debug - Raw breadcrumb attribute:', breadcrumbAttr ? breadcrumbAttr.substring(0, 100) + '...' : 'null');
                    
                    if (!breadcrumbAttr) {
                        return [];
                    }
                    
                    // Decode HTML entities - handle more cases
                    let decodedAttr = breadcrumbAttr
                        .replace(/&quot;/g, '"')
                        .replace(/&amp;/g, '&')
                        .replace(/&lt;/g, '<')
                        .replace(/&gt;/g, '>')
                        .replace(/&#39;/g, "'");
                    
                    console.log('GTM Debug - Decoded breadcrumb:', decodedAttr.substring(0, 100) + '...');
                    
                    const parsed = JSON.parse(decodedAttr);
                    console.log('GTM Debug - Parsed breadcrumb data:', parsed);
                    return parsed;
                } catch (e) {
                    console.warn('GTM Debug - Error parsing breadcrumb data:', e);
                    console.warn('GTM Debug - Raw attribute value:', headerComponent.getAttribute('breadcrumb'));
                    return [];
                }
            } else {
                console.log('GTM Debug - No breadcrumb attribute found');
                return [];
            }
        }
        
        // Function to get page category based on location and breadcrumbs
        function getPageCategory() {
            if (window.location.pathname === '/') {
                return 'Home';
            }
            
            // Get breadcrumb data from em-header component
            const breadcrumbData = getBreadcrumbData();
            if (breadcrumbData.length > 1) {
                const lastBreadcrumb = breadcrumbData[1];
                return lastBreadcrumb.Name || "homepageTemplate";
            }
            
            return "homepageTemplate"; // Fallback to server-side value
        }
        
        // Function to get page name based on location and breadcrumbs
        function getPageName() {
            if (window.location.pathname === '/') {
                return 'Home';
            }
            
            // Get breadcrumb data from em-header component
            const breadcrumbData = getBreadcrumbData();
            if (breadcrumbData.length > 2) {
                const lastBreadcrumb = breadcrumbData[breadcrumbData.length - 1];
                return lastBreadcrumb.Name || "Home";
            }
            
            return "Home"; // Fallback to server-side value
        }
        
        
        // Function to initialize GTM tracking when DOM is ready
